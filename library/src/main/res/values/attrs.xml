<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckView">
        <attr name="android:checked" />
        <attr name="checkedColor" format="color" />
        <attr name="borderColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="numberValue" format="integer" />
        <attr name="drawableValue" format="reference" />
        <attr name="drawableSize" format="dimension" />
        <attr name="checkMode">
            <enum name="drawableMode" value="0" />
            <enum name="numberMode" value="1" />
        </attr>
    </declare-styleable>
</resources>