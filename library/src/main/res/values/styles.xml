<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--图片文件夹pop动画-->
    <style name="FloderPopAnimStyle">
        <item name="android:windowEnterAnimation">@anim/imagepicker_pop_slide_in</item>
        <item name="android:windowExitAnimation">@anim/imagepicker_pop_slide_out</item>
    </style>

    <style name="ImagePickerCheckBoxStyle">
        <item name="android:textSize">@dimen/image_picker_text_size_medium</item>
        <item name="android:button">@drawable/selector_image_picker_checkbox</item>
        <item name="android:padding">@dimen/image_picker_padding_horizontal_tiny</item>
        <item name="android:colorControlHighlight">@android:color/transparent</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

</resources>